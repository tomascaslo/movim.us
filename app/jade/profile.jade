div(class="container")
	img(ng-src="{{ user.profileImage }}" class= "img-responsive img-circle displayed resize profile" alt = "Responsive image")
	div(class="text-center")
		h3 {{ user.name }}
	//- checkar alingment
	div(class="row")
		div(class="col-md-6")
			h4(class="pull-right") {{ user.score }}%
		div(class="col-md-6")
			img(src="images/thumbs-up.jpg" class="pull-left img-responsive displayed resize-thumb")
	div(class="col-md-2 bold")
		p Recent
	hr
	div(ng-repeat="task in tasks")
		task-element-directive